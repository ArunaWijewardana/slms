{% extends 'base.html.twig' %}

{% block body %}
    <h1>{{ 'Create Payment Category'|trans }}</h1>

    <ul>
        <a href="{{ path('paymentcategory_index') }}" class="btn btn-secondary">{{ 'Back to the list'|trans }}</a>
    </ul>

    <div class="form-group">
        {{ form_start(form) }}
            {{ form_widget(form) }}
            <input type="submit" value="{{ 'Create'|trans }}" class="form-control btn btn-success" />
